{
    "version": 2,
    "builds": [
        {
            "src": "api/",
            "use": "@vercel/node"
        },
        {
            "src": "client/dist",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/api(.*)",
            "dest": "/api/index.js"
        }
    ]
}