{
    "version": 2,
    "builds": [
        {
            "src": "api",
            "use": "@vercel/node",
            "config": {
                "build": "index.js"
            }
        },
        {
            "src": "client",
            "use": "@vercel/static",
            "routes": [
                {
                    "src": "/(.*)",
                    "dest": "/index.html"
                }
            ]
        }
    ]
}